nation.js","original":{"line":105,"column":99},"generated":{"line":1,"column":1458}},{"source":"components/Pagination.js","name":"currentPage","original":{"line":105,"column":109},"generated":{"line":1,"column":1467}},{"source":"components/Pagination.js","name":"maxPages","original":{"line":105,"column":124},"generated":{"line":1,"column":1470}},{"source":"components/Pagination.js","original":{"line":104,"column":6},"generated":{"line":1,"column":1473}},{"source":"components/Pagination.js","name":"paginationOutput","original":{"line":110,"column":9},"generated":{"line":1,"column":1481}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1483}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1487}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1489}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1491}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1499}},{"source":"components/Pagination.js","name":"Pagination","original":{"line":113,"column":15},"generated":{"line":1,"column":1507}}],"sources":{"components/Pagination.js":"import React from 'react';\nimport PaginationItem from './PaginationItem.js';\nimport useDataContext from '../context/useDataContext';\nimport useCore from '../methods/core/useCore';\n\nfunction Pagination() {\n  const {\n    pageNext, \n    pagePrev, \n    scrollUp\n  } = useCore();\n\n  const {\n    currentPage,\n    maxPages,\n    setAfterFirstEvent\n  } = useDataContext();\n\n  const maxPageItems = 3;\n  let paginationOutput = '';\n\n  function clickNext() {\n    scrollUp();\n    pageNext();\n    setAfterFirstEvent(true);\n  }\n\n  function clickPrev() {\n    scrollUp();\n    pagePrev();\n    setAfterFirstEvent(true);\n  }\n\n  const pageItems = function(amount) {\n    amount = amount > maxPages ? maxPages : amount;\n\n    const middle = Math.round(amount/2);\n    const list = [];\n    const firstPages = currentPage > 0 && currentPage < amount;\n    const lastPages = currentPage <= maxPages && currentPage > maxPages - (amount - 1);\n    let output;\n    let first = '';\n    let last = '';\n\n    for(let i = 0; i < amount; i++) {\n      //Beginning\n      if (firstPages) {\n        output = 1 + i;\n      }\n\n      //End\n      else if (lastPages) {\n        output = (maxPages - amount) + (1 + i);\n      }\n\n      //Middle\n      else { \n        output = currentPage - (middle - (i + 1));\n      }\n\n      if (output > 0 && output <= maxPages) {\n        list.push(\n          <PaginationItem \n            key={i} \n            pageNumber={output} \n          ></PaginationItem>\n        );\n      }\n    }\n\n    if(!firstPages && maxPages > list.length) {\n      first = <PaginationItem \n        pageNumber={1}\n        className={'first-item'}\n      ></PaginationItem>\n    }\n\n    if (!lastPages && maxPages > list.length) {\n      last = <PaginationItem \n        pageNumber={maxPages}\n        className={'last-item'}\n      ></PaginationItem>\n    }\n\n    return (\n      <li className=\"eight29-pagination-list\">\n        <ul>\n          {first}\n          {list}\n          {last}\n        </ul>\n      </li>\n    );\n  };\n\n  if (maxPages > 1) {\n    paginationOutput = <ul className=\"eight29-pagination\">\n      <li className=\"eight29-pagination-prev\">\n        <button className=\"eight29-pagination-arrow\" title=\"Previous Page\" onClick={() => clickPrev()} disabled={currentPage <= 1}>&lt;</button>\n      </li>\n\n      {pageItems(maxPageItems)}\n\n      <li className=\"eight29-pagination-next\">\n        <button className=\"eight29-pagination-arrow\" title=\"Next Page\" onClick={() => clickNext()} disabled={currentPage >= maxPages}>&gt;</button>\n      </li>\n    </ul>\n  }\n\n  return paginationOutput;\n}\n\nexport default Pagination;"},"lineCount":null}},"error":null,"hash":"08d95bd8c0a034c13737dad821120b6e","cacheData":{"env":{}}}