e":118,"column":15},"generated":{"line":1,"column":1551}},{"source":"components/filters/FilterSelect.js","name":"FilterSelect","original":{"line":118,"column":15},"generated":{"line":1,"column":1559}}],"sources":{"components/filters/FilterSelect.js":"import React from 'react';\nimport PropTypes from 'prop-types';\nimport FilterContainer from './FilterContainer';\nimport useSettingsContext from '../../context/useSettingsContext';\nimport useDataContext from '../../context/useDataContext';\nimport useCore from '../../methods/core/useCore';\n\nfunction FilterSelect(props) {\n  const {\n    taxonomy,\n    taxSlug,\n    label,\n    collapsible,\n    scrollable\n  } = props;\n\n  const {selected} = useDataContext();\n  const {displayPostCounts} = useSettingsContext();\n  const {replaceSelected} = useCore();\n  const allTerms = [];\n  const filterId = taxSlug ? `filter-${taxSlug}` : '';\n  let parentContent;\n  let childContent;\n\n  FilterSelect.propTypes = {\n    taxonomy: PropTypes.array,\n    taxSlug: PropTypes.string,\n    label: PropTypes.string,\n    collapsible: PropTypes.bool,\n    scrollable: PropTypes.bool\n  }\n\n  function changeValue(value) {\n    if (value === 'empty') {\n      replaceSelected('', taxSlug);\n    }\n\n    else {\n      replaceSelected(value, taxSlug);\n    }\n  }\n\n  if (taxonomy) {\n    allTerms.push(\n        <option \n          key={`empty-${taxSlug}`}\n          value={'empty'}\n          id={`empty-${taxSlug}`}\n        >Select {label}\n        </option>\n    );\n\n    taxonomy.forEach(term => {\n      if (displayPostCounts) {\n        parentContent = `${term.name} (${term.count})`;\n      }\n      else {\n        parentContent = term.name;\n      }\n\n      if (!term.hide_term) {\n        allTerms.push(\n          <option \n            key={term.id}\n            value={`${term.id}`}\n            id={`${taxSlug}-${term.id}`}\n          >\n          {parentContent}\n          </option>\n        )\n      }\n\n      if (term.children) {\n        term.children.forEach(child => {\n          if (displayPostCounts) {\n            childContent = `${child.name} (${child.count})`;\n          }\n          else {\n            childContent = child.name;\n          }\n\n          if(!term.hide_term) {\n            allTerms.push(\n              <option \n                key={child.id}\n                value={`${child.id}`}\n                id={`${taxSlug}-${child.id}`}\n              >\n              {childContent}\n              </option>\n            )\n          }\n        });\n      }\n    });\n  }\n\n  return (\n    <FilterContainer \n    className=\"filter-select\"\n    filterId={filterId}\n    label={label}\n    collapsible={collapsible}\n    scrollable={scrollable}\n    >\n      <select\n        id={`${filterId}-input`}\n        multiple={false}\n        value={selected[taxSlug]}\n        onChange={(e) => {changeValue(e.target.value)}}\n      >\n        {allTerms}\n      </select>\n    </FilterContainer>\n  )\n}\n\nexport default FilterSelect;"},"lineCount":null}},"error":null,"hash":"272a9360735cdf4b12b10dc3a7d99e00","cacheData":{"env":{}}}